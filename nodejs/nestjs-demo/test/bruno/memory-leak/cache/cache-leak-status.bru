meta {
  name: cache-leak-status
  type: http
  seq: 2
}

get {
  url: {{host}}/memory-leak/cache/status
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.isLeaking: isBoolean
  res.body.stats: isDefined
  res.body.stats.size: isNumber
  res.body.stats.memoryUsage: isString
  res.body.stats.maxSize: isNumber
  res.body.message: isString
}
