meta {
  name: event-leak-status
  type: http
  seq: 3
}

get {
  url: {{host}}/memory-leak/event/status
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.message: isDefined
  res.body.stats: isDefined
  res.body.stats.activeListeners: isNumber
  res.body.stats.totalMemoryAllocated: isNumber
  res.body.stats.isLeaking: isBoolean
}
