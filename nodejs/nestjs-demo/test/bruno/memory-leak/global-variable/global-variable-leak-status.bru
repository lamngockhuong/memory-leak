meta {
  name: global-variable-leak-status
  type: http
  seq: 2
}

get {
  url: {{host}}/memory-leak/global-variable/status
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.stats: isDefined
  res.body.stats.leakedArrays: isNumber
  res.body.stats.estimatedMemoryMB: isNumber
  res.body.stats.isLeaking: isBoolean
}
